{
	"name": "offlineextension",
	"version": "0.0.0",
	"author": [
		"Srijan Agarwal"
	],
	"url": "",
	"descriptionmsg": "offlineextension-desc",
	"license-name": "MIT",
	"type": "other",
	"AutoloadClasses": {
		"offlineextensionHooks": "offlineextension.hooks.php",
		"SpecialHelloWorld": "specials/SpecialHelloWorld.php"
	},
	"config": {
		"offlineextensionEnableFoo": true
	},
	"ExtensionMessagesFiles": {
		"offlineextensionAlias": "offlineextension.i18n.alias.php"
	},
	"Hooks": {
		"NameOfHook": [
			"offlineextensionHooks::onNameOfHook"
		]
	},
	"MessagesDirs": {
		"offlineextension": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.offlineextension.foo": {
			"scripts": [
				"modules/ext.offlineextension.js",
				"modules/ext.offlineextension.foo.js"
			],
			"styles": [
				"modules/ext.offlineextension.foo.css"
			],
			"messages": [],
			"dependencies": []
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "offlineextension"
	},
	"SpecialPages": {
		"HelloWorld": "SpecialHelloWorld"
	},
	"manifest_version": 1
}
